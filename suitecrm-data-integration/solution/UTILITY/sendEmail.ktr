    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Script here

var sec, min, hour, DURATION_STRING, START_DATE_STRING, END_DATE_STRING;

START_DATE_STRING = date2str(ETL_START_DATE,"dd/MM/yyyy HH:mm:ss");
END_DATE_STRING = date2str(ETL_END_DATE,"dd/MM/yyyy HH:mm:ss");

if(ETL_DURATION&lt;3600){
    var a = Math.floor(ETL_DURATION/60); //minutes
    var b = ETL_DURATION%60; //seconds

    if (b!=1){
        sec = "seconds";
    }else{
        sec = "second";
    }

    if(a!=1){
        min = "minutes";
    }else{
        min = "minute";
    }

	DURATION_STRING = a+" "+min+" "+b+" "+sec

}else{
        var a = Math.floor(ETL_DURATION/3600); //hours
    var x = ETL_DURATION%3600;
    var b = Math.floor(x/60); //minutes
    var c = ETL_DURATION%60; //seconds

     if (c!=1){
        sec = "seconds";
    }else{
        sec = "second";
    }

    if(b!=1){
        min = "minutes";
    }else{
        min = "minute";
    }

    if(c!=1){
        hour = "hours";
    }else{
        hour = "hour";
    }

	DURATION_STRING = a+" "+hour+", "+b+" "+min+" "+c+" "+sec

}

</jsScript_script>
      </jsScript>
    </jsScripts>
